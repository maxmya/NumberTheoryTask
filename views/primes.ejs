<link rel='stylesheet' href='/stylesheets/style.css' />
<h2>
  Task 1 </h2>
<form method="POST">
  <input name="number" type="number">
  <button type="submit">Check</button>
</form>
<div class="cont">
  <% if (result) { %>
    <div class="narrow"> is prime? :</div>
    <div class="wide">
      <%= result.isPrime %>
    </div>
    <div class="narrow"> number of Primes in the range of 0 to x is : </div>
    <div class="wide">
      <%= result.numberOfPrimes %>
    </div>
    <div class="narrow"> Prime List : </div>
    <div class="wide">
      <p>
        <%= result.primeList %>
      </p>
    </div>
    <div class="narrow"> Primes of Form [4k+1] : </div>

    <div class="wide">
      <%= result["4k+1"] %>
    </div>
    <div class="narrow">Primes of Form [4k+3] : </div>
      <div class="wide">
        <%= result["4k+3"] %>
      </div>
      <% } %>
    </div>